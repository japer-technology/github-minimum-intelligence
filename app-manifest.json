{
  "name": "github-minimum-intelligence",
  "description": "An AI agent that lives in your GitHub repository",
  "url": "https://github.com/japer-technology/github-minimum-intelligence",
  "public": true,
  "default_permissions": {
    "issues": "write",
    "contents": "write",
    "actions": "write",
    "metadata": "read"
  },
  "default_events": [
    "issues",
    "issue_comment",
    "installation",
    "installation_repositories"
  ]
}
